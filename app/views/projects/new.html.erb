<h2>New Project</h2>

<%= form_for(@project) do |f| %>
	<div class="form-group">
		<%= f.label :name %>
		<%= f.text_field :name, class: "form-control" %>
	</div>
	<div class="form-group">
		<%= f.label :description %>
		<%= f.text_field :description, class: "form-control" %>
	</div>
	<div class="form-group">
		<%= f.submit %>
	</div>
<% end %>